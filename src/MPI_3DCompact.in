#Inputs for MPI_3DCompact...

#If running from a restart file
[RESTART]
FROMRESTART = FALSE
ONLYGRIDFROMRESTART = FALSE
FILENAME = SolutionDump.5

SPONGEAVGFROMRESTART = FALSE
SPONGEAVGFILENAME = SpongeAvgDump.5

#Information about stepping...
[TIMESTEPPING]
TSTYPE	    = CONST_CFL
#DT	    = 0.001
CFL 	    = 1.0
MAXTIME     = 10000.0
MAXTIMESTEP = 75000
FILTERSTEP  = 3
CHECKSTEP   = 1
DUMPSTEP    = 5000

#Statistics recording stuff...
[STATS]
INTERVAL = 2

VELOCITYSTATS = 1
VELOCITYSTATSFROMRESTART = 0
VELOCITYSTATSFILENAME = VelocityStats.5

THERMOSTATS = 0
THERMOSTATSFROMRESTART = 0
THERMOSTATSFILENAME = ThermoStats.5

STATSAVGTYPE = XI3_AVG


#Boundary Condition Information
[BC]
BCXTYPE = PERIODIC_SOLVE
BCYTYPE = DIRICHLET_SOLVE
BCZTYPE = PERIODIC_SOLVE

BCX0 = INTERNALLY_PERIODIC
BCX1 = INTERNALLY_PERIODIC
BCY0 = ADIABATIC_WALL
BCY1 = SPONGE
BCZ0 = PERIODIC
BCZ1 = PERIODIC

PERIODICDISPZ_X = 0
PERIODICDISPZ_Y = 0
PERIODICDISPZ_Z = 3.1416

#Stuff for sponge boundary conditions
[SPONGE]
KIND = CYLINDRICAL
CYLAXIS_ORIENT = 2
CYLAXIS_X = 0
CYLAXIS_Y = 0
CYLAXIS_Z = 0
RMIN = 9
STRENGTH = 2.0

#Mesh size information if not from a restart
[DOMAIN]
NX = 200
NY = 140
NZ = 48
#NX = 25
#NY = 25
#NZ = 25

#MPI decomposition of the mesh, double zeros tries to figure out the best
[PENCILDECOMP]
PROW = 0
PCOL = 0

#Solver specific items
[SOLVER]
ALPHAF = 0.25
MU_REF = 0.0003
USETIMING = FALSE
XFDTYPE = CD2
YFDTYPE = CD2
ZFDTYPE = CD2
FILTERTYPE = COMPACT8
